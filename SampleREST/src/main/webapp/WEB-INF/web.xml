  </servlet>
  <servlet>
    <servlet-name>StampaLettera</servlet-name>
    <servlet-class>it.contrada.servlets.StampaLetteraPDF</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ContradaWebMaven</servlet-name>
    <servlet-class>org.glassfish.jersey.servlet.ServletContainer</servlet-class>
    <init-param>
      <param-name>jersey.config.server.provider.packages</param-name>
      <param-value>it.contrada.bean.rest</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>ContradaWebMaven</servlet-name>
    <url-pattern>/rest/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Generate PDF</servlet-name>
    <url-pattern>*.stampa</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Faces Servlet</servlet-name>
    <url-pattern>*.faces</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Blocking Servlet</servlet-name>
