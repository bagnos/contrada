  </context-param>
  <context-param>
    <param-name>com.icesoft.faces.portlet.renderStyles</param-name>
    <param-value>true</param-value>
  </context-param>
  <context-param>
    <param-name>statoDefault</param-name>
    <param-value>IT</param-value>
  </context-param>
  <context-param>
    <param-name>provinciaNascitaDefault</param-name>
    <param-value>52</param-value>
  </context-param>
  <context-param>
    <param-name>provinciaResidenzaDefault</param-name>
    <param-value>52</param-value>
