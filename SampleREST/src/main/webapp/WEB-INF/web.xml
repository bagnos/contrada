    <param-name>comuneResidenzaDefault</param-name>
    <param-value>32</param-value>
  </context-param>
  <context-param>
    <param-name>capResidenzaDefault</param-name>
    <param-value>53100</param-value>
  </context-param>
  <context-param>
    <param-name>incasso</param-name>
    <param-value>1</param-value>
  </context-param>
  <context-param>
    <param-name>tipoTessera</param-name>
    <param-value>1</param-value>
  </context-param>
